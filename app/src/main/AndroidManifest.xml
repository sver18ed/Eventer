<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eventer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PendingFriendRequestsActivity"></activity>
        <activity android:name=".ViewFriendsListActivity" />
        <activity android:name=".CreateAccount" />
        <activity android:name=".ViewProfileActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ViewEventActivity" />
        <activity android:name=".EditEventActivity" />
        <activity android:name=".CreateEventActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>